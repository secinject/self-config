[global_config]
[keybindings]
  zoom_in = <Primary>equal
  zoom_out = <Primary>minus
  zoom_normal = <Primary>0
  new_tab = <Primary><Shift>t
  cycle_next = ""
  split_horiz = <Primary>Down
  split_vert = <Primary>Up
  copy = <Primary>c
  paste = <Primary>v
  page_up = <Shift><Alt>Up
  page_down = <Shift><Alt>Down
  page_up_half = <Shift>Up
  page_down_half = <Shift>Down
  next_tab = <Primary>Tab
[profiles]
  [[default]]
    cursor_color = "#aaaaaa"
    scrollback_infinite = True
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
[plugins]
